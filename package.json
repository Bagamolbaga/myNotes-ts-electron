{
  "name": "vite-reactts-electron-starter",
  "version": "0.5.0",
  "license": "MIT",
  "main": "main/index.js",
  "author": {
    "name": "Maximilian St√ºmpfl"
  },
  "description": "Starter using Vite 2+ React +Typescript + Electron for fast prototyping.",
  "keywords": [
    "vite",
    "react",
    "electron",
    "typescript"
  ],
  "repository": {
    "url": "https://github.com/lTimeless/vite-reactts-electron-starter"
  },
  "productName": "vite-reactts-electron-starter",
  "scripts": {
    "dev": "concurrently \"npm run dev:vite\" \" npm run dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "npm run build:electron && electron .",
    "build": "npm run build:vite && npm run build:electron",
    "build:vite": "vite build",
    "build:electron": "tsc -p electron",
    "dist": "npm run build && electron-builder",
    "pack": "npm run build && electron-builder --dir",
    "clean": "rimraf dist main src/out",
    "type-check": "tsc",
    "lint": "eslint . --ext js,jsx,ts,tsx",
    "lint:fix": "eslint . --ext js,jsx,ts,tsx --fix"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.15",
    "@types/md5": "^2.3.1",
    "@types/react-redux": "^7.1.19",
    "@types/react-router-dom": "^5.3.1",
    "@uiw/react-markdown-editor": "^3.2.5",
    "@uiw/react-markdown-preview": "^3.4.0",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "axios": "^0.23.0",
    "firebase": "^9.1.2",
    "jwt-decode": "^3.1.2",
    "md5": "^2.3.0",
    "node-sass": "^6.0.1",
    "react": "^17.0.2",
    "react-bootstrap": "^1.6.4",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.5",
    "react-router-dom": "^5.3.0",
    "redux": "^4.1.1",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.3.0",
    "sass": "^1.42.1",
    "socket.io-client": "^4.2.0",
    "vite": "^2.6.2",
    "@types/node": "^16.10.2",
    "@types/react": "^17.0.26",
    "@types/react-dom": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "4.32.0",
    "@typescript-eslint/parser": "4.32.0",
    "autoprefixer": "^10.3.6",
    "concurrently": "^6.3.0",
    "electron-is-dev": "^2.0.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "postcss": "^8.3.8",
    "prettier": "2.4.1",
    "tailwindcss": "^2.2.16",
    "typescript": "^4.4.3"
  },
  "devDependencies": {
    "electron": "^15.1.0",
    "electron-builder": "^22.10.5"
  },
  "build": {
    "appId": "my-app",
    "win": {
      "target":["nsis"],
      "icon": "src/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "installerIcon": "src/icon.ico",
      "uninstallerIcon": "src/icon.ico",
      "uninstallDisplayName": "myNotes-uninstaller",
      "license": "LICENSE.md",
      "allowToChangeInstallationDirectory": true
    }
  }
}
